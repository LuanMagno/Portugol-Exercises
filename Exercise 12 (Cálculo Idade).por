algoritmo "Sua idade"
var
   ano_atual,ano_nascimento,subtracao_anos:real
inicio
      Escreva("Em que ano nós estamos?")
      Leia(ano_atual)
      Escreva("Em que ano eu nasci?")
      Leia(ano_nascimento)
    subtracao_anos <- ano_atual - ano_nascimento
    Escreva(subtracao_anos, " anos de idade")
fimalgoritmo