algoritmo "semnome"
var
   Valor,Contador:real
inicio
      Escreva("Digite um valor positivo: ")
      Leia(Valor)
      Contador<- 1
      Enquanto (Contador<=Valor) faca
               Escreva(Contador)
               Contador<- Contador+1
      FimEnquanto
      Escreva(" Acabou!")
fimalgoritmo